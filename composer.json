{
    "name": "imrealashu/laravelrest",
    "description": "Simple Package to create Laravel REST API",
    "keywords": [
        "laravel",
        "Laravel Rest"
    ],
    "homepage": "https://github.com/imrealashu/laravelrest",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashish Singh",
            "email": "imrealashu@gmail.com",
            "homepage": "https://github.com/imrealashu",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "imrealashu\\laravelrest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "imrealashu\\laravelrest\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
