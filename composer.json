{
    "name": ":imrealashu/laravel-rest",
    "description": "Simple REST API Transformer for Laravel 5",
    "keywords": [
        "imrealashu",
        "laravel-rest"
    ],
    "homepage": "https://github.com/imrealashu/laravel-rest",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashish Singh",
            "email": "imrealashu@gmail.com",
            "homepage": "https://github.com/imrealashu",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            ":vendor\\:package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
